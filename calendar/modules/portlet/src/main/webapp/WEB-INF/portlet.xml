<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  id="oppna-program-notessystem-calendar-module-portlet">

  <portlet>
    <portlet-name>NotesCalendar</portlet-name>
    <display-name>Mina Kalendrar</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
    </supports>
    <supported-locale>sv</supported-locale>
    <resource-bundle>se.vgregion.portal.notessystem.calendar</resource-bundle>
    <portlet-info>
      <title>Mina kalendrar</title>
      <short-title>Mina kalendrar</short-title>
      <keywords>notes kalender calendar</keywords>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>MyDayCalendar</portlet-name>
    <display-name>Min Dag</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
    </supports>
    <supported-locale>sv</supported-locale>
    <resource-bundle>se.vgregion.portal.notessystem.calendar</resource-bundle>
    <portlet-info>
      <title>Min Dag</title>
      <short-title>Min Dag</short-title>
      <keywords>notes kalender calendar agenda</keywords>
    </portlet-info>
  </portlet>

  <!-- Used to get remote user. See more info in JSR-286 Section 21.1. -->
  <user-attribute>
    <description>User Login Id</description>
    <name>user.login.id</name>
  </user-attribute>

  <default-namespace>http://notes-calendar/</default-namespace>
</portlet-app>
